{
  "name": "get_fit",
  "version": "0.2.0",
  "description": "Fitness Tracker for MyFitnessPal",
  "main": "app.js",
  "scripts": {
    "up:dev": "docker-compose -f docker-compose.dev.yml up --abort-on-container-exit --force-recreate",
    "build:dev": "docker-compose -f docker-compose.dev.yml build",
    "down:dev": "docker-compose -f docker-compose.dev.yml down -v",
    "up:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit --force-recreate",
    "build:test": "docker-compose -f docker-compose.test.yml build",
    "down:test": "docker-compose -f docker-compose.test.yml down -v",
    "up:prod": "docker-compose -f docker-compose.prod.yml up --force-recreate",
    "build:prod": "docker-compose -f docker-compose.prod.yml build",
    "down:prod": "docker-compose -f docker-compose.prod.yml down -v",
    "push": "docker push samhinshaw/get_fit:$DOCKER_TAG",
    "pull": "docker pull samhinshaw/get_fit:$DOCKER_TAG",
    "dev:server": "nodemon -L -e js,pug src/app.js --exec babel-node",
    "dev:ui": "webpack --watch --mode development",
    "test:once": "babel-node tests/config/run-tests.js",
    "test:watch": "nodemon -L -e js,pug tests/config/run-tests.js --exec babel-node",
    "clean:ui": "rm -rf ./dist",
    "transpile": "babel ./src -d ./dist",
    "bundle": "webpack --mode production",
    "lint:js": "eslint --ignore-path .eslintignore --config .eslintrc.js \"**/*.js\"",
    "format:js": "npm run lint:js --fix",
    "format:prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write \"**/*.{yml,json,html,md,css,scss}\"",
    "run:prod": "node dist/app.js",
    "deploy": "pm2 deploy getfit.config.js production",
    "rollbar": "curl https://api.rollbar.com/api/1/deploy/ -F access_token=607807c2e15542d3bf8d2111b4fe5282 -F environment=production -F revision=`git log -n 1 --pretty=format:'%H'` -F local_username=`whoami`",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samhinshaw/get_fit.git"
