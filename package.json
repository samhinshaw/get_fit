{
  "name": "get_fit",
  "version": "1.0.0",
  "description": "Fitness Tracker for Sam & Am",
  "main": "app.js",
  "scripts": {
    "dev": "webpack -d && nodemon lib/app.js --exec babel-node",
    "clean": "rm -rf ./dist",
    "build": "babel ./lib -d ./dist -s && webpack -p",
    "uglifyClient":
      "uglifyjs public/src/js/client.js --compress --mangle --source-map --output public/dist/js/bundle.min.js",
    "format": "prettier --list-different --single-quote --print-width=120 \"public/js/**/*.js\"",
    "lint": "eslint **/*.js --quiet",
    "prod": "node dist/app.js",
    "test": "snyk test"
  },
