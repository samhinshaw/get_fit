{
  "name": "get_fit",
  "version": "1.0.0",
  "description": "Fitness Tracker for Sam & Am",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon lib/app.js --exec babel-node",
    "watch": "NODE_ENV=development webpack -d -w",
    "webpack": "NODE_ENV=production webpack -p",
    "clean": "rm -rf ./dist",
    "build": "NODE_ENV=production babel ./lib -d ./dist -s && webpack -p",
    "format": "prettier --list-different --single-quote --print-width=120 \"public/js/**/*.js\"",
    "lint": "eslint **/*.js --quiet",
    "deploy": "pm2 deploy getfit.config.js production",
    "prod": "NODE_ENV=production node dist/app.js",
    "test": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samhinshaw/get_fit.git"
  },
  "author": "Sam Hinshaw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samhinshaw/get_fit/issues"
  },
  "homepage": "https://github.com/samhinshaw/get_fit#readme",
