    "clean": "rm -rf ./dist",
    "build": "NODE_ENV=production babel ./src -d ./dist && webpack -p",
    "format": "prettier --write --single-quote --print-width=100 'src/**/*.js' 'public/src/js/**/*.js'",
    "lint": "eslint **/*.js",
    "deploy": "pm2 deploy getfit.config.js production",
    "prod": "NODE_ENV=production node dist/app.js",
    "test": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect; npm run snyk-protect",
