{
  "name": "get_fit",
  "version": "1.0.0",
  "description": "Fitness Tracker for Sam & Am",
  "main": "app.js",
  "scripts": {
    "start": "uglifyjs public/js/client.js --compress --mangle --source-map --output public/js/bundle.min.js && grunt dev && nodemon lib/app.js --exec babel-node",
    "build": "rm -rf ./dist && babel ./lib -d ./dist -s",
    "uglifyClient": "uglifyjs public/js/client.js --compress --mangle --source-map --output public/js/bundle.min.js",
    "format": "prettier --list-different --single-quote --print-width=120 \"public/js/**/*.js\"",
    "lint": "eslint **/*.js --quiet",
    "prod": "uglifyjs public/js/client.js --compress --mangle --source-map --output public/js/bundle.min.js && grunt dev && node dist/app.js",
    "test": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samhinshaw/get_fit.git"
  },
  "author": "Sam Hinshaw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samhinshaw/get_fit/issues"
  },
  "homepage": "https://github.com/samhinshaw/get_fit#readme",
  "dependencies": {
    "ajv": "^5.2.4",
    "ajv-keywords": "^2.1.0",
    "apollo-client-preset": "^1.0.6",
    "apollo-server-express": "^1.3.2",
    "async": "^2.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express": "^4.15.4",
    "express-brute": "^1.0.1",
    "express-brute-mongoose": "^0.0.9",
    "express-graphql": "^0.6.11",
    "express-messages": "^1.0.1",
    "express-sanitizer": "^1.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.1.1",
    "google-spreadsheet": "^2.0.4",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "helmet": "^3.9.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "moment-range": "^3.0.3",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.11.11",
    "mongoose-schema-to-graphql": "^2.7.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "python-shell": "^0.4.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-node8": "^1.2.0",
    "eslint": "^4.7.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^3.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "prettier": "^1.7.3",
    "snyk": "^1.49.2",
    "uglify-es": "^3.1.2"
  }
}
