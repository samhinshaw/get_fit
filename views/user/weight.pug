extends ../partials/layout

block header
  include ../includes/user_header

block subnav
  include ../includes/user_subnav

block content
  //- Bring in custom stylesheet for D3 chart
  link(rel='stylesheet', href='/css/d3.css')
  //- D3 Chart Placeholder
  #chart
  table
    thead
      th date
      th weight
    each row in sheetRows
      tr
        td #{row.date}
        td #{row.weight}
